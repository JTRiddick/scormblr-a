{
  "name": "Scormblr-A",
  "version": "0.0.1",
  "description": "Redux Express Webpack Isomorphic ",
  "main": "server.js",
  "scripts": {
    "start:x": "npm run build && export UNIVERSAL=true && babel-node src/run-server.js",
    "start": "npm run build && babel-node src/run-server.js",
    "start:dev": "export NODE_ENV=development && npm run build:dev && nodemon --exec babel-node -- src/server.js",
    "start:universal": "export UNIVERSAL=true && npm run start",
    "start:dev:universal": "export NODE_ENV=development && export UNIVERSAL=true && npm run start:dev",
    "build": "NODE_ENV=production webpack -p",
    "build:dev": "webpack -d",
    "build:dev:watch": "webpack -d --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-form": "^6.8.0",
    "redux-promise": "^0.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "chai": "^4.0.2",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "http-server": "^0.10.0",
    "jsdom": "^11.0.0",
    "mocha": "^3.4.2",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "promise": "^7.1.1",
    "react-addons-test-utils": "^15.5.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.6.1",
    "webpack-split-by-path": "^2.0.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": "off"
    }
  }
}
